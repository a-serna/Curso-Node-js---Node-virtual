{{>header}}
<!-- Default form login -->

<div class="row">
	<div class="col-lg-6 mx-auto mt-5 mb-5">
		<form class="text-center border border-light p-5">

    <p class="h4 mb-4">Ingresar</p>

    <!-- Email -->
    <input type="Nombre" id="defaultLoginFormname" class="form-control mb-4" placeholder="Nombre" required>

    <!-- Password -->
    <input type="Contraseña" id="defaultLoginFormPassword" class="form-control mb-4" placeholder="Contraseña" required>

    <!-- Sign in button -->
    <button class="btn btn-info btn-block my-4" type="submit">Ingresar</button>

    <!-- Register -->
    <p>Not a member?
        <a href="/registro">Registrarme</a>
    </p>

</form>
	</div>
</div>

{{>footer}}
